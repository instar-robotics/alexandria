<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<script>
    <name>Lms</name>	
    <rt_token uuid="100"  unit="ms">10</rt_token> 
    <functions>
	<function uuid="1">
		<name>MSum</name>   
		<libname>arithmetic</libname>
		<publish topic="incond">true</publish>
		<save>true</save>          
                <output type="MATRIX">
			<rows>2</rows>
			<cols>2</cols>
		</output>
		<inputs>	
			<input type="SCALAR_MATRIX" uuid="2" multiple="true">
				<name>inMatrix</name>
				<links>
					<link uuid="3" constant="true" secondary="false">
						<weight>1</weight>
						<from>0</from>
					</link>
					<link uuid="4" constant="false" secondary="true" >
						<from>1</from>
						<weight>0.998</weight>
					</link>
				</links>	
			</input>
		</inputs>	
	</function>
	<function uuid="6">
                <name>LMS</name>
		<libname>lms</libname>
		<publish topic="lms_out">true</publish>      <!-- default : false -->
                <output type="MATRIX">
			<rows>2</rows>
			<cols>2</cols>
		</output>
                <inputs>
                        <input type="SCALAR_SCALAR" uuid="7">
                                <name>learning_rate</name>
				<links>
					<link uuid="8"  constant="true" secondary="false">
						<weight>0.1</weight>
						<from>0</from>
					</link>
				</links>	
                        </input>
                        <input type="SCALAR_MATRIX" uuid="9">
                                <name>unconditionnal</name>
				<links>
					<link uuid="10" constant="false" secondary="false">
						<from>1</from>
						<weight>1</weight>
					</link>
				</links>	
                        </input>
                        <input type="MATRIX_MATRIX" uuid="11" multiple="true">
                                <name>conditionnals</name>
				<links>
					<link uuid="12" constant="true" secondary="false">
						<weight>0.001</weight>
						<connectivity type="ONE_TO_ALL"></connectivity>
						<from>0</from>
					</link>
					<link uuid="101" constant="true" secondary="false">
						<weight>0.001</weight>
						<connectivity type="ONE_TO_ALL"></connectivity>
						<from>0</from>
					</link>
				</links>	
                        </input>
                </inputs>
        </function>
    </functions>
</script>
