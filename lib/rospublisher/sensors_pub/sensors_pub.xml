<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<description>
	<libname>sensors_pub</libname>
	<functions>
		<function>
			<name>ImuPub</name>
                        <output type="MATRIX" shape="VECT">
                        </output>
                        <inputs>
                                <input  type="STRING">
                                        <name>topic_name</name>
                                        <desc>Name of the topic to which the function publish</desc>
                                </input>
                                <input  type="SCALAR_SCALAR">
                                        <name>size_queue</name>
                                        <desc>Size of the queue</desc>
                                </input>
                                <input  type="SCALAR_MATRIX" checkSize="false">
                                        <name>orientation</name>
					<desc>orientation</desc>
                                </input>
                                <input  type="SCALAR_MATRIX" checkSize="false">
                                        <name>angular_velocity</name>
                                        <desc>angular velocity</desc>
                                </input>
                                <input  type="SCALAR_MATRIX" checkSize="false">
                                        <name>linear_acceleration</name>
                                        <desc>linear acceleration</desc>
                                </input>
                        </inputs>
                        <icon></icon>
                        <desc>Publish sensor_msgs::Imu message</desc>
                </function>
	</functions>
</description>
